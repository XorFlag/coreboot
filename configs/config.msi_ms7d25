CONFIG_LOCALVERSION="v1.1.0"
CONFIG_VENDOR_MSI=y
CONFIG_CBFS_SIZE=0x1000000
CONFIG_CONSOLE_CBMEM_BUFFER_SIZE=0x100000
CONFIG_EDK2_BOOT_TIMEOUT=3
CONFIG_PCIEXP_DEFAULT_MAX_RESIZABLE_BAR_BITS=35
CONFIG_BOARD_MSI_Z690_A_PRO_WIFI_DDR4=y
CONFIG_EDK2_BOOTSPLASH_FILE="bootsplash.bmp"
CONFIG_POWER_STATE_OFF_AFTER_FAILURE=y
CONFIG_UART_PCI_ADDR=0x0
CONFIG_PCIEXP_SUPPORT_RESIZABLE_BARS=y
CONFIG_SUBSYSTEM_VENDOR_ID=0x0000
CONFIG_SUBSYSTEM_DEVICE_ID=0x0000
CONFIG_I2C_TRANSFER_TIMEOUT_US=500000
CONFIG_SMMSTORE_SIZE=0x40000
CONFIG_DISPLAY_FSP_TIMESTAMPS=y
CONFIG_TPM2=y
CONFIG_TPM_MEASURED_BOOT=y
CONFIG_DEFAULT_CONSOLE_LOGLEVEL_0=y
